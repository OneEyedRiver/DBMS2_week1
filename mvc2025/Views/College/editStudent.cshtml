@model mvc2025.Models.College.student
@{
    ViewData["Title"] = "Edit Student";

}

<h1 class="red"> Edit Student</h1>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-group">
        <label>Student Number</label> <br>
        @Html.EditorFor(x => x.student_no, new { htmlAttribute = new { @class = "form-control col-md-5" } })

    </div>

    <div class="form-group">
        <label>Name</label> <br>
        @Html.EditorFor(x => x.name, new { htmlAttribute = new { @class = "form-control col-md-5" } })

    </div>

    <div class="form-group">
        <label>Course</label> <br>
        @Html.EditorFor(x => x.course, new { htmlAttribute = new { @class = "form-control col-md-5" } })

    </div>

    <div class="form-group">
        <label>Address</label> <br>
        @Html.EditorFor(x => x.address, new { htmlAttribute = new { @class = "form-control col-md-5" } })

    </div>

    <div class="form-group">
        <input type="submit" value="Update" class="btn btn-default" />
    </div>

}


